{
  "version": 2,
  "builds": [
   {
    "src": "server.js",
    "use": "@vercel/node",
    "config": { "includeFiles": ["dist/**"] }
   }
  ],
  "routes": [
   {
    "src": "/(.*)",
    "dest": "server.js"
   }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://beauty-company-project.vercel.app"
        }
      ]
    }
  ]
 }